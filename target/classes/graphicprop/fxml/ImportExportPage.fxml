<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="700.0" prefWidth="1000.0" stylesheets="@../stylesheets/ImportExport.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.controller.ImportExportPageView">
   <children>
      <AnchorPane prefHeight="700.0" prefWidth="500.0" styleClass="leftPane">
         <children>
            <Pane layoutX="75.0" layoutY="175.0" prefHeight="350.0" prefWidth="350.0" style="-fx-background-color: orange;">
               <children>
                  <ImageView fitHeight="306.0" fitWidth="231.0" layoutY="105.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/egyptMan.png" />
                     </image>
                  </ImageView>
                  <Button fx:id="importButton" layoutX="257.0" layoutY="163.0" onMouseClicked="#run" onMouseEntered="#hoverAnimation" mnemonicParsing="false" text="Import" />
               </children>
            </Pane>
         </children></AnchorPane>
      <AnchorPane layoutX="500.0" prefHeight="700.0" prefWidth="500.0" styleClass="rightPane">
         <children>
            <Pane layoutX="75.0" layoutY="175.0" prefHeight="350.0" prefWidth="350.0" style="-fx-background-color: gray;">
               <children>
                  <ImageView fitHeight="342.0" fitWidth="246.0" layoutX="104.0" layoutY="89.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/egyptWoman.png" />
                     </image>
                  </ImageView>
                  <Button fx:id="exportButton" layoutX="27.0" layoutY="163.0" onMouseClicked="#run" onMouseEntered="#hoverAnimation" mnemonicParsing="false" text="Export" />
               </children>
            </Pane>
         </children></AnchorPane>
   </children>
</AnchorPane>
