<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<AnchorPane fx:id="pane" prefHeight="700.0" prefWidth="1000.0" styleClass="pane"
            stylesheets="/graphicprop/stylesheets/RegisterPage.css" xmlns="http://javafx.com/javafx/16"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.controller.RegisterView">
    <Pane layoutX="42.0" layoutY="113.0" prefHeight="200.0" prefWidth="200.0">
        <Label layoutX="72.0" layoutY="14.0" text="WELCOME"/>
        <Button fx:id="Register" onMouseClicked="#run" layoutX="14.0" layoutY="86.0" mnemonicParsing="false"
                prefHeight="25.0" prefWidth="80.0"
                text="Register"/>
        <Button fx:id="Login" onMouseClicked="#run" layoutX="104.0" layoutY="85.0" mnemonicParsing="false"
                prefHeight="26.0" prefWidth="80.0"
                text="Login"/>
        <Button fx:id="Exit" onMouseClicked="#run" layoutX="60.0" layoutY="124.0" mnemonicParsing="false"
                prefHeight="25.0" prefWidth="80.0" text="Exit"/>
    </Pane>
</AnchorPane>
