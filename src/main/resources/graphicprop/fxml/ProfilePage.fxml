<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="pane" prefHeight="700.0" prefWidth="1000.0" stylesheets="/graphicprop/stylesheets/ProfilePage.css"
            xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="view.controller.ProfileView">

    <Button fx:id="ChangeNickname" layoutX="444.0" layoutY="277.0" onMouseClicked="#run"
            onMouseEntered="#hoverAnimation" text="Change Nickname"/>
    <Button fx:id="ChangePassword" layoutX="446.0" layoutY="317.0" onMouseClicked="#run"
            onMouseEntered="#hoverAnimation" text="Change Password"/>
    <Button fx:id="Exit" layoutX="483.0" layoutY="361.0" onMouseClicked="#run" onMouseEntered="#hoverAnimation"
            text="Exit"/>
    <Label fx:id="TopicProfile" alignment="CENTER" layoutX="441.0" layoutY="193.0" prefHeight="53.0" prefWidth="119.0"
           text="Profile Menu" textAlignment="CENTER"/>
</AnchorPane>
