<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="700.0" prefWidth="1000.0" stylesheets="@../stylesheets/MainPage.css"
            xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="view.controller.MainMenuView">
    <ImageView fitHeight="125.0" fitWidth="254.0" layoutX="36.0" layoutY="52.0" pickOnBounds="true"
               preserveRatio="true">
        <Image url="@../images/YuGiOhLogo.png"/>
    </ImageView>
    <Button fx:id="Duel" onMouseClicked="#run" layoutX="44.0" layoutY="176.0" mnemonicParsing="false" prefHeight="58.0"
            prefWidth="238.0" text="DUEL"/>
    <Button fx:id="DecksConstruction" onMouseClicked="#run" layoutX="44.0" layoutY="252.0" mnemonicParsing="false"
            prefHeight="58.0" prefWidth="238.0" text="Decks Construction"/>
    <Button fx:id="Shop" onMouseClicked="#run" layoutX="44.0" layoutY="326.0" mnemonicParsing="false" prefHeight="37.0"
            prefWidth="238.0" text="Shop"/>
    <Button fx:id="Setting" onMouseClicked="#run" layoutX="44.0" layoutY="399.0" mnemonicParsing="false"
            prefHeight="40.0" prefWidth="238.0" text="Setting"/>
    <Button fx:id="Logout" onMouseClicked="#run" layoutX="44.0" layoutY="473.0" mnemonicParsing="false"
            prefHeight="30.0" prefWidth="238.0" text="Logout"/>
</AnchorPane>
